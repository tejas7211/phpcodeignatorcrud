    document.write("-----parsefloat-----" + "<br>");
    //parses a value as a string and returns the first number
    document.write(parseFloat(10) + "<br>" +  
    parseFloat("10") + "<br>" +
    parseFloat("10.33") + "<br>" +
    parseFloat("34 45 66") + "<br>" +
    parseFloat("He was 40") + "<br>" +
