    //object arrays
    let stud = [{
    "name" : "tejas",
    "age" : 22,
    "college" : "dcs",
    "roll_no" : 12,
    // "favmovies" : ["dhoom","hum"],
    // "living" : {
    //     "city" : "surat",
    //     "country" : "india"
    },
    {
    "name" : "fenil",
    "age" : 23,
    "college" : "vnsgu"
    },
    {
    "name" : "sagar",
    "age" : 22,
    "college" : "iit"
    }
];


stud[0].name = "mayu";   //add value after declare array
console.log(stud);  //print in console

for(let i=0;i<stud.length;i++){    //print in document
    document.write(stud[i].name + " " + stud[i].age +  " " + stud[i].college + "<br>");
}

stud[1].school = "abc school";  //add new value
console.log(stud);

let exmplobj = {            //add new array objact
    "name" : "harsh",
    "age" : 28,
    "college" : "gtu"
}
stud.push(exmplobj);
console.log(stud);  //push

stud.pop();
console.log(stud);  //pop 

stud.shift();
console.log(stud);  //shift

stud.unshift(exmplobj);
console.log(stud);  //unshift

console.log(stud.length);  //length

console.log(stud.slice(1,3));  //slice  //1st type
stud.slice(1,3,exmplobj);  //2nd type
console.log(stud);

stud.splice(1,exmplobj);  //splice--
console.log(stud);

console.log(stud.reverse());  //reverse 

console.log(stud.sort((a)=>a.name)); //sort

stud.valueOf();  //valueof
console.log(stud);  

function isfind(detail){   //find
    return detail.name == "fenil";
}
console.log(stud.find(isfind));
// console.log(stud.find((a)=>isfind(a)));

function isfilter(detail){   //filter
    return detail.name === "harsh";
}
console.log(stud.filter(isfilter));
  
// var findex = stud.map(function (elem){  //findIndex
//     return elem.name;
// }).findIndex("fenil");
// console.log(findex);

let index = stud.findIndex((obj)=>obj.name=="fenil");
console.log(index);

var indexoff = stud.map(function (elem){  //indexOf
    return elem.name;
}).indexOf("sagar");
console.log(indexoff);

var lindex = stud.map(function (elem){  //LastIndexOf
    return elem.name;
}).lastIndexOf("harsh");
console.log(lindex);

console.log(stud.map(function(elem){  //join
    return elem.name;
}).join(","));

const mp = stud.map((obj)=>obj.age==22);   //map return until lenth like true false
console.log(" map result is - " , mp);

const mp1 = stud.filter((obj)=>obj.age==22);  //filter return anything  like all element
console.log(" filter result is - " , mp1);

const mp2 = stud.findIndex((obj)=>obj.age==22);
console.log(" findindex result is - " , mp2);

