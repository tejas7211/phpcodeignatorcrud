let arr = [10, 30, 44, 50, 59, 45, 70, 60];


function findNumber(number) {
  60 <= number 
  ? console.log(number + " is greate or equal to 60") 
  : 45 <= number &&  59 >= number 
  ? console.log(number + " is equal or between 45 and 59") 
  : console.log(number + " is less than or equal to 44") 
}

for(let i=0; i < arr.length; i++ ) {
  findNumber(arr[i])
}
